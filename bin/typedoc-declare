#!/usr/bin/env node

const package = require('../package.json');

const isVersion = process.argv.find(arg => arg === '-v' || arg === '--version');
if (isVersion) {
  console.log(`${package.name} ${package.version}`);
}

const cli = require('typedoc/dist/lib/cli');
const app = new cli.CliApplication({
  plugin: package.name,
});

