{
  "name": "typedoc-plugin-typescript-declaration",
  "version": "0.0.1",
  "description": "Typedoc plugin to render to typescript declaration file",
  "main": "dist/index.js",
  "scripts": {
    "start": "ts-node src/index",
    "build": "rimraf dist && rollup -c",
    "test:watch": "jest --watch",
    "doc": "typedoc --out output/docs --includeDeclarations --mode file --excludeExternals src/index.ts",
    "test": "jest --verbose"
  },
  "keywords": [
    "typedocplugin",
    "typedoc",
    "typescript",
    "declaration",
    "d.ts"
  ],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/glob": "^7.1.1",
    "@types/jest": "^24.0.25",
    "glob": "^7.1.6",
    "jest": "^24.9.0",
    "make-dir": "^3.0.0",
    "rimraf": "^3.0.0",
    "rollup": "^1.27.14",
    "rollup-plugin-typescript2": "^0.25.3",
    "ts-jest": "^24.2.0",
    "ts-node": "^8.5.4",
    "typescript": "^3.7.4"
  },
  "dependencies": {
    "typedoc": "^0.15.6"
  }
}
